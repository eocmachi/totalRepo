스크롤내렸을 때 요소 올라오기
                $(document).scroll(function() {
        var y = $(this).scrollTop();
        if (y > 1200) {
          $('.taste-title').addClass('on');
        } else {
          $('.taste-title').removeClass('on');
        }
      });
    $(document).scroll(function() {
        var y = $(this).scrollTop();
        if (y > 1200) {
          $('.taste-title').addClass('on');
        } else {
          $('.taste-title').removeClass('on');
        }
      });


